{
	"name": "esbuild-plugin-esmfile",
	"version": "0.1.1",
	"description": "esbuild plugin to build *.<file-extension>.css.ts/*.<file-extension>.css.js files as *.<file-extension> file assets",
	"keywords": [
		"binary",
		"css",
		"file",
		"esm",
		"esbuild",
		"md",
		"svg"
	],
	"homepage": "https://github.com/esmfile/esbuild-plugin-esmfile#readme",
	"bugs": {
		"url": "https://github.com/esmfile/esbuild-plugin-esmfile/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/esmfile/esbuild-plugin-esmfile.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"_fixtures"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"esbuild": "^0.20.1"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}